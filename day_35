CREATE TABLE orders ( order_id INT PRIMARY KEY, company VARCHAR(50), amount DECIMAL(10, 2)
);
INSERT INTO orders (order_id, company, amount) VALUES
(1, 'Zomato', 500.00),
(2, 'Swiggy', 300.00),
(3, 'Zomato', 700.00), 
(4, 'Swiggy', 200.00), 
(5, 'Zomato', 1000.00);



Find the total revenue generated by Zomato orders.


select company, sum(amount) as total_revenue
from orders
where company = "Zomato"
group by 1;
